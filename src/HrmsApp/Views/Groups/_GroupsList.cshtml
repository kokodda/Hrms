@using HrmsModel.Models;
@model IEnumerable<GenericGroup>

<div class="container-fluid" style="padding:0px">
    <div>
        <a role="button" href="#" class="btn btn-xs" onclick="addGroup()" data-toggle="modal" data-target="#myModal" title="Add Group"><span class="glyphicon glyphicon-plus"></span></a>&nbsp;
        <span><label><small>Groups List</small></label></span>
    </div>
    <br />
    <ul>
        @foreach(var item in Model)
        {
            <li><a href="#" onclick="subGroupsList(@item.Id)"><small>@item.Name</small></a></li>
        }
    </ul>
</div>